{
	"name": "ollama-d",
	"description": "D bindings for the Ollama API",
	"license": "MIT",
	"version": "0.2.0",
	"targetPath": "lib",
	"authors": [
		"Matheus Catarino FranÃ§a"
	],
	"dependencies": {
		"vibe-d": "~>0.10.2"
	},
	"dflags": [
		"-preview=dip1000",
		"-preview=rvaluerefparam"
	],
	"dflags-dmd": [
